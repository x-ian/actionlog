<h1>Listing priority_axes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% for priority_axis in @priority_axes %>
  <tr>
    <td><%=h priority_axis.name %></td>
    <td><%=h priority_axis.description %></td>
    <td><%= link_to 'Show', priority_axis %></td>
    <td><%= link_to 'Edit', edit_priority_axis_path(priority_axis) %></td>
    <td><%= link_to 'Destroy', priority_axis, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New priority_axis', new_priority_axis_path %>
