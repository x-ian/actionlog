<h1>Editing priority_axis</h1>

<% form_for(@priority_axis) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @priority_axis %> |
<%= link_to 'Back', priority_axes_path %>
