<h1>Listing priority_ranges</h1>

<table>
  <tr>
    <th>Value</th>
    <th>Description</th>
    <th>Priority axis</th>
  </tr>

<% for priority_range in @priority_ranges %>
  <tr>
    <td><%=h priority_range.value %></td>
    <td><%=h priority_range.description %></td>
    <td><%=h priority_range.priority_axis_id %></td>
    <td><%= link_to 'Show', priority_range %></td>
    <td><%= link_to 'Edit', edit_priority_range_path(priority_range) %></td>
    <td><%= link_to 'Destroy', priority_range, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New priority_range', new_priority_range_path %>
